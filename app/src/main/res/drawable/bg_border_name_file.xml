<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" >
            <corners android:radius="@dimen/_20sdp"/>
            <gradient
                android:angle="90"
                android:endColor="@color/color_3278C3"
                android:startColor="@color/color_4AC789" />
            <size android:width="24dp"
                android:height="24dp"/>
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <corners android:radius="@dimen/_20sdp"/>
            <solid android:color="#011D23" />
        </shape>
    </item>

</layer-list>