<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btfContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_bg_bottom_navigation"
        tools:ignore="SpUsage,ContentDescription">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btf_firstAction"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toStartOf="@+id/btf_secondAction"
            app:layout_constraintStart_toStartOf="parent"
            android:visibility="gone">

            <ImageView
                android:id="@+id/btf_firstIcon"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/ic_bottom_bar_topic_selector"
                app:layout_constraintBottom_toTopOf="@+id/btf_firstTitle"
                app:layout_constraintDimensionRatio="1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.428571429"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/btf_firstTitle"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/_1sdp"
                android:ellipsize="marquee"
                android:fontFamily="@font/font_regular"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/txt_topic"
                android:textColor="@color/color_bottom_bar_selector"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.285714286"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btf_firstIcon" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btf_secondAction"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toEndOf="@+id/btf_threeAction"
            app:layout_constraintStart_toEndOf="@+id/btf_firstAction">

            <ImageView
                android:id="@+id/btf_secondIcon"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/ic_bottom_bar_home_selector"
                app:layout_constraintBottom_toTopOf="@+id/btf_secondTitle"
                app:layout_constraintDimensionRatio="1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.428571429"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/btf_secondTitle"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/_1sdp"
                android:ellipsize="marquee"
                android:fontFamily="@font/font_regular"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/txt_chat_ai"
                android:textColor="@color/color_bottom_bar_selector"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.285714286"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btf_secondIcon" />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btf_threeAction"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toStartOf="@+id/btf_fourAction"
            app:layout_constraintStart_toEndOf="@+id/btf_secondAction">

            <ImageView
                android:id="@+id/btf_threeIcon"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/ic_bottom_bar_art_selector"
                app:layout_constraintBottom_toTopOf="@+id/btf_threeTitle"
                app:layout_constraintDimensionRatio="1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.428571429"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/btf_threeTitle"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/_1sdp"
                android:ellipsize="marquee"
                android:fontFamily="@font/font_regular"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/midj_art"
                android:textColor="@color/color_bottom_bar_selector"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.285714286"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btf_threeIcon" />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btf_fourAction"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/btf_threeAction">

            <ImageView
                android:id="@+id/btf_fourIcon"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:src="@drawable/ic_bottom_bar_setting_selector"
                app:layout_constraintBottom_toTopOf="@+id/btf_fourTitle"
                app:layout_constraintDimensionRatio="1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.428571429"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed" />

            <TextView
                android:id="@+id/btf_fourTitle"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/_1sdp"
                android:ellipsize="marquee"
                android:fontFamily="@font/font_regular"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/text_setting"
                android:textColor="@color/color_bottom_bar_selector"
                android:textSize="@dimen/_10sdp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.285714286"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btf_fourIcon" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>